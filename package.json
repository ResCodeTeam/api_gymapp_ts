{
  "name": "api_gymapp_ts",
  "version": "1.0.0",
  "description": "Api em ts de suporte à aplicação gym app",
  "main": "app.js",
  "scripts": {
    "start": "node dist/app.js",
    "build": "tsc",
    "postinstall": "npm run build",
    "docs": "npx typedoc --cleanOutputDir --out docs src/api/**/**/*.ts src/api/**/**/**/*.ts src/**.ts",
    "dev": "nodemon --ignore ./src/tests ./src/app.ts",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/**/*.ts'",
    "test_agendamentos": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/agendamentos/aluno/*.ts'",
    "test_agendamentos_treinador": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/agendamentos/treinador/*.ts'",
    "test_noti": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha --timeout 10000 -r ts-node/register './src/tests/notificacoes/*.ts'",
    "test_def": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/definicoes/*.ts'",
    "test_treinos": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/treinos/*.ts'",
    "test_ati": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/atividades/*.ts'",
    "test_musc": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/musculos/*.ts'",
    "test_ad": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/admin/*.ts'",
    "test_perfil": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/perfil/*.ts'",
    "test_marcas": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/marcas/*.ts'",
    "test_alunos": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/alunos/*.ts'",
    "test_loc": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/localMedida/*.ts'",
    "test_treinadores": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/treinadores/*.ts'",
    "test_avaliacao": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/avaliacao/*.ts'",
    "test_plano": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/plano/*.ts'",
    "test_exercicios": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/exercicios/*.ts'",
    "test_ginasios": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/ginasios/*.ts'",
    "test_posts": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/posts/*.ts'",
    "test_modalidades": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register './src/tests/modalidades/*.ts'"
  },
  "author": "ResCode",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.14.0-dev.28",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "bcrypt": "^5.0.1",
    "commonjs": "^0.0.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.23",
    "@types/swagger-ui-express": "^4.1.3",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "chai-things": "^0.2.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "prisma": "^3.14.0-dev.28",
    "ts-node": "^10.7.0",
    "typescript": "^4.7.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ResCodeTeam/api_gymapp_ts.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/ResCodeTeam/api_gymapp_ts/issues"
  },
  "homepage": "https://github.com/ResCodeTeam/api_gymapp_ts#readme"
}
